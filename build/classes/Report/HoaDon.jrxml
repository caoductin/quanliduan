	<queryString>
		<![CDATA[SELECT quanliduan.chitiethoadon.`ID`,
	quanliduan.chitiethoadon.`MaNV`,
	quanliduan.chitiethoadon.`MaChiTietHoaDon`,
	quanliduan.chitiethoadon.`MaSanPham`,
	quanliduan.chitiethoadon.`SoLuong`,
